vehicles = {
    'Dream': 'Honda 250T',
    'roadster': 'BMW R1100',
    'er5': 'Kawasaki ER5',
    'can-am': 'Bombardier can-am 250',
    'virago': 'Yamaha XV250',
    'tenere': 'Yamaha XT650',
    'jimny': 'Suzuki jimny 1.5',
    'fiesta': 'Ford Fiesta Ghia 1.4'
}

my_car = vehicles['roadster']                           # indexing method
print(my_car)

commuter = vehicles['virago']
print(commuter)

leaner = vehicles.get('ER5')                            # dot get method
print(leaner)
