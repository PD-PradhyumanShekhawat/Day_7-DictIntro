vehicles = {
    'Dream': 'Honda 250T',
    'roadster': 'BMW R1100',
    'er5': 'Kawasaki ER5',
    'can-am': 'Bombardier can-am 250',
    'virago': 'Yamaha XV250',
    'tenere': 'Yamaha XT650',
    'jimny': 'Suzuki jimny 1.5',
    'fiesta': 'Ford Fiesta Ghia 1.4'
}

for key in vehicles:                              # not so efficient way
  print(key, vehicles[key], sep=', ')

for key, values in vehicles.items():              # More efficient way
  print(key, values, sep=", ")
